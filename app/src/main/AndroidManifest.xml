<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.harsh.dentalcare">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ChairsActivity"></activity>
        <activity android:name=".AppointmentActivity" />
        <activity android:name=".AddTreatmentActivity" />
        <activity android:name=".TableActivity" />
        <activity android:name=".TreatmentActivity" />
        <activity android:name=".HistoryActivity" />
        <activity
            android:name=".AddPatient"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AddDoctor"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>